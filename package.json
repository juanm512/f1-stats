{
  "name": "f1-stats",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "date-fns": "^2.29.3",
    "expo": "~48.0.6",
    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-image": "~1.0.0",
    "expo-linear-gradient": "~12.1.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-router-native": "^6.8.2",
    "expo-splash-screen": "~0.18.1",
    "react-native-web": "~0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "standard": "^17.0.0"
  },
  "private": true,
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "rules": {
      "space-before-function-paren": "off",
      "quotes": [
        "error",
        "double"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "react/react-in-jsx-scope": "off"
    }
  }
}
